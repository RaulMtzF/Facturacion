    <!-- inject:css -->
    @Styles.Render("~/assets/css/shared/style.css")
    <!-- endinject -->
    <!-- Layout styles -->
    @Styles.Render("~/assets/css/demo_1/style.css")
    <!-- End Layout styles -->
    @Styles.Render("~/assets/images/favicon.ico")
</head>
<body>
