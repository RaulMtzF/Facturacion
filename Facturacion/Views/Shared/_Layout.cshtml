    @Styles.Render("~/assets/vendors/iconfonts/flag-icon-css/css/flag-icon.min.css")
    @Styles.Render("~/assets/vendors/css/vendor.bundle.base.css")
    @Styles.Render("~/assets/vendors/css/vendor.bundle.addons.css")
    <!-- endinject -->
    <!-- plugin css for this page -->
